{
  "$schema": "http://json.schemastore.org/template",
  "author": "YourName",
  "classifications": ["Rust", "Plugin", "uMod", "Oxide"],
  "identity": "RustPlugin.Template",
  "name": "Rust Plugin Template",
  "shortName": "rustplugin",
  "sourceName": "RustPluginName",
  "preferNameDirectory": true,
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "symbols": {
    "PluginAuthor": {
      "type": "parameter",
      "replaces": "PluginAuthor",
      "defaultValue": "YourName"
    },
    "PluginDescription": {
      "type": "parameter",
      "replaces": "PluginDescription",
      "defaultValue": "Description of your plugin"
    }
  }
} 