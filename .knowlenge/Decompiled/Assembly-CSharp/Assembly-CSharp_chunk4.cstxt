		TimeWarning val = TimeWarning.New("ThinkMissions", 0);
		try
		{
			ValidEnumerator<BasePlayer> enumerator = playerCache.ValidPlayers.GetEnumerator();
			try
			{
				while (enumerator.MoveNext())
				{
					BasePlayer current = enumerator.Current;
					if (current.timeSinceMissionThink < current.thinkEvery)
					{
						current.timeSinceMissionThink += delta;
						continue;
					}
					foreach (BaseMission.MissionInstance mission in current.missions)
					{
						try
						{
							mission.Think(current, current.timeSinceMissionThink);
						}
						catch (Exception ex)
						{
							Debug.LogException(ex);
						}
					}
					current.timeSinceMissionThink = 0f;
				}
			}
			finally
			{
				((IDisposable)enumerator/*cast due to .constrained prefix*/).Dispose();
			}
		}
		finally
		{
			((IDisposable)val)?.Dispose();
		}
	}

