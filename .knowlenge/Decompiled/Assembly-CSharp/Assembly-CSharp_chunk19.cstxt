		{
			value = basePlayer.UserIDString;
			text = basePlayer.displayName;
		}
		else if (PrefabAttribute.server.Find(killer.prefabID, out result) && !string.IsNullOrEmpty(result.title?.english))
		{
			value = "";
			text = result.title.english;
		}
