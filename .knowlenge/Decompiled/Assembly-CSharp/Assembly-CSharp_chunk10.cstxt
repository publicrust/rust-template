			}
			Write(3735928559u);
		}

		public void Write(string text)
		{
			Write((ushort)text.Length);
			for (int i = 0; i < text.Length; i++)
			{
				stream.WriteByte((byte)text[i]);
			}
		}

		public void Write(ulong value)
		{
			for (byte b = 0; b < 8; b++)
			{
				stream.WriteByte((byte)(value >> 56 - b * 8));
			}
		}

		public void Write(uint value)
		{
