		}
		return result;
	}

	public void Return(T[] array)
	{
		if (!_buffers.TryGetValue(array.Length, out var value))
		{
			value = new ConcurrentQueue<T[]>();
			_buffers[array.Length] = value;
		}
		value.Enqueue(array);
	}
}


