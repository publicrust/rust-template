			if (string.Equals(text, b, StringComparison.InvariantCultureIgnoreCase))
			{
				Debug.LogWarning((object)("Ferry schedule for '" + zone + "' has the same zone twice in a row: " + scheduleString));
				entries = null;
				return false;
			}
		}
		entries = array;
		return true;
	}

