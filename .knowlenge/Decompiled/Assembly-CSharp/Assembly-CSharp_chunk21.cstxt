					Debug.LogWarning((object)ex3.ToString());
				}
				else
				{
					Debug.LogException(ex3);
				}
			}
			finally
			{
				Pool.Free<EventRecord>(ref records, true);
				Pool.FreeUnmanaged(ref stream);
			}
		}
	}

