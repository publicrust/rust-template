			Legs = new List<Leg>();
		}
		if ((bool)Mecanim && Mecanim.isHuman)
		{
			Finder_AutoFindLegsIfHuman(Mecanim);
		}
		else if (Legs.Count < 2)
		{
			Finder_EnsureLegsCount(2);
		}
