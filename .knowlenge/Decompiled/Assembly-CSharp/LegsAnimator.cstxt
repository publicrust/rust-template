							duringLegAdjustMovement = true;
						}
						else
						{
							animationMoveType = EMoveType.FromAnimation;
							_legSpherizeLocalVector = Vector3.zero;
							duringLegAdjustMovement = false;
						}
					}
					else
					{
						duringLegAdjustMovement = false;
						transitionProgress = 0f;
					}
