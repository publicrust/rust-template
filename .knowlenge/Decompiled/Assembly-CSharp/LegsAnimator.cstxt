						}
						Vector3 vector = previousPositionWorld;
						Vector3 vector2 = leg.ankleAlignedOnGroundHitWorldPos - vector;
						float magnitude = vector2.magnitude;
						legMoveDistanceFactor = magnitude / (Owner.ScaleReference * 0.6f);
