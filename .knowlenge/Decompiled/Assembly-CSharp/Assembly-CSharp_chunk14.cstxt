			key = list[index].Key;
		}
		Pool.FreeUnmanaged<NexusZoneDetails>(ref list);
		return key;
	}

	private void GetStarterZones(List<NexusZoneDetails> zones)
	{
		NexusZoneClient zoneClient = ZoneClient;
		object obj;
		if (zoneClient == null)
		{
			obj = null;
